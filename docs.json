{
  "$schema": "https://mintlify.com/docs.json",
  "name": "Wavedash Developer Docs",
  "theme": "aspen",
  "colors": {
    "primary": "#6366F1",
    "light": "#818CF8",
    "dark": "#4F46E5"
  },
  "fonts": {
    "family": "GT America",
    "source": "/fonts/GT-America-Standard-Regular.woff2",
    "format": "woff2",
    "weight": 400
  },
  "favicon": "/favicon.svg",
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Getting Started",
        "groups": [
          {
            "group": "Introduction",
            "pages": ["quickstart"]
          }
        ]
      },
      {
        "tab": "CLI",
        "groups": [
          {
            "group": "CLI Reference",
            "pages": [
              "cli/index",
              "cli/auth",
              "cli/dev",
              "cli/build",
              "cli/configuration",
              "cli/troubleshooting"
            ]
          }
        ]
      },
      {
        "tab": "SDK",
        "groups": [
          {
            "group": "Getting Started",
            "pages": ["sdk/index", "sdk/initialization"]
          },
          {
            "group": "Features",
            "pages": [
              "sdk/leaderboards",
              "sdk/lobbies",
              "sdk/p2p",
              "sdk/achievements",
              "sdk/ugc",
              "sdk/remote-storage",
              "sdk/presence"
            ]
          },
          {
            "group": "Reference",
            "pages": ["sdk/types", "sdk/signals"]
          }
        ]
      },
      {
        "tab": "Engines",
        "groups": [
          {
            "group": "Integration Guides",
            "pages": ["engines/godot", "engines/unity", "engines/custom"]
          }
        ]
      }
    ]
  },
  "navbar": {
    "links": [
      {
        "label": "Discord",
        "href": "https://discord.gg/bBaWUjTJWx"
      },
      {
        "label": "Support",
        "href": "mailto:developer@wavedash.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://wavedash.com/developers/dashboard"
    }
  },
  "contextual": {
    "options": ["copy", "view", "chatgpt", "claude"]
  },
  "footer": {
    "socials": {
      "discord": "https://discord.gg/bBaWUjTJWx",
      "github": "https://github.com/wvdsh",
      "x": "https://x.com/wavedash"
    }
  }
}
